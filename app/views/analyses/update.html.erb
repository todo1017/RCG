<% flash.each do |key, value| %>
  <% if value != '' %>
    <div class="alert alert-<%= key %>"><%= value %></div>
  <% end %>
<% end %>
<div id="#analyses-update">
	<div class="panel panel-default">
		<div class="panel-heading"> Current State </div>
		<div class="panel-body">
			<p>
				<div class="col-xs-6 col-sm-2">New</div>
				<div class="col-xs-6 col-sm-10 count-new">+ <%= $new_count %></div>
			</p>
			<br/>
			<p>
				<div class="col-xs-6 col-sm-2">Updated</div>
				<div class="col-xs-6 col-sm-10 count-deleted">/ <%= $updated_count %></div>
			</p>
			<br/>
			<p>
				<div class="col-xs-6 col-sm-2">Deleted</div>
				<div class="col-xs-6 col-sm-10 count-deleted">- <%= $deleted_count %></div>
			</p>
		</div>
	</div>
	<%= link_to 'Update', "/analyses/update_data", class: "btn btn-default" %>
</div>