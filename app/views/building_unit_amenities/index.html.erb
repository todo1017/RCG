<p id="notice"><%= notice %></p>

<h1>Listing Building Unit Amenities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @building_unit_amenities.each do |building_unit_amenity| %>
      <tr>
        <td><%= link_to 'Show', building_unit_amenity %></td>
        <td><%= link_to 'Edit', edit_building_unit_amenity_path(building_unit_amenity) %></td>
        <td><%= link_to 'Destroy', building_unit_amenity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

